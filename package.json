{
    "name": "azure-env-generator",
    "version": "0.1.3",
    "description": "Generate .env files from Azure Variable Groups.",
    "author": "Griffiths Waite",
    "license": "MIT",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "typings": "dist/index.d.cts",
    "type": "module",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            },
            "default": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/griffiths-waite/azure-env-generator"
    },
    "keywords": [],
    "scripts": {
        "build": "tsup",
        "lint": "tsc",
        "test": "vitest --coverage",
        "release": "pnpm build && changeset publish",
        "dev": "pnpm lint && node dist/cli/bin.js"
    },
    "bin": {
        "azure-env-generator": "dist/cli/bin.js"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.12",
        "@types/node": "^24.2.1",
        "@vitest/coverage-istanbul": "^3.0.5",
        "prettier": "^3.6.2",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3",
        "vitest": "^3.0.5"
    },
    "dependencies": {
        "dotenv": "^17.2.1"
    }
}
